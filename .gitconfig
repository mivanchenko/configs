[user]
        name = User Name
        email = user@email.com
[color]
        status = auto
        branch = auto
        interactive = auto
        diff = auto
[push]
        default = simple
[merge]
        ff = false
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
